{
    "language": "English",
    "rules": {
        "absolute": {
            "$pronoun month": "~in (?<pronoun>this|next|last) month~",
            "$year-$month-$day": "~in (?<month>january|february|march|april|may|june|july|august|september|october|november|december)~",
            "$hour:$min:$sec": "~((at )?(?<hour>\\d{1,2})\\:(?<min>\\d{2})(\\:(?<sec>\\d{2}))?)~",
            "$pronoun week $weekday": "~(on )?(?<pronoun>next|this|last) (?<weekday>monday|tuesday|wednesday|thursday|friday|saturday|sunday)~",
            "$year-$month-$digit": "~((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) (?<month>january|february|march|april|may|june|july|august|september|october|november|december)( (?<year>[[:digit:]]{4})( year)?)?~",
            "$pronoun year": "~(?<pronoun>this|next|last) year(?!s)~",
            "$digit-$month-$day": "~(?<digit>[[:digit:]]+) year(?!s)~",
            "$pronoun week": "~(?<pronoun>this|next|last) week~"
        },
        "relative": {
            "@relative": "~(in(?<relative>((((?<!in)(\\sand|,))?\\s(\\d+|\\pL+(\\s\\pL+)?))\\s(hours?|minutes?|seconds?|years?|weeks?|days?|months?)){2,}))~",
            "@relative_split": "~(\\sand|,)\\s~",
            "@relative_join": "in",
            "+$1 hour": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) hours?)~",
            "+$1 minute": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) minutes?)~",
            "+$1 sec": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) seconds?)~",
            "+$1 year": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) years?)~",
            "+$1 week": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) weeks?)~",
            "+$1 day": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) days?)~",
            "+$1 month": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) months?)~",
            "-$1 hour": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?hours? ago)~",
            "-$1 minute": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?minutes? ago)~",
            "-$1 sec": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?seconds? ago)~",
            "-$1 year": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?years? ago)~",
            "-$1 week": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?weeks? ago)~",
            "-$1 day": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?days? ago)~",
            "-$1 month": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?months? ago)~",
            "+2 day": "~\\bday after tomorrow\\b~",
            "+1 day": "~\\btomorrow\\b~",
            "-2 day": "~\\bthe day before yesterday\\b~",
            "-1 day": "~\\byesterday\\b~"
        }
    },
    "pronouns": {
        "this": "this",
        "next": "next",
        "last": "last"
    },
    "week_days": {},
    "months": {},
    "units": {
