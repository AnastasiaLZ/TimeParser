{
	"language": "English",
	"rules": {
		"absolute": {
			"month": "~in ((?<pronoun>this|next) month|(?<month>january|february|march|april|may|june|july|august|september|october|november|december))~",
			"time": "~((at )?(?<hour>\\d{1,2})\\:(?<min>\\d{2})(\\:(?<sec>\\d{2}))?)~",
			"weekday": "~(on )?(?<pronoun>next|this) (?<weekday>monday|tuesday|wednesday|thursday|friday|saturday|sunday)~",
			"date": "~((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) (?<month>january|february|march|april|may|june|july|august|september|october|november|december)( (?<year>[[:digit:]]{4})( year)?)?~",
			"year": "~((?<pronoun>this|next)|((?<digit>[[:digit:]]+))) year(?!s)~u",
			"week": "~(?<pronoun>this|next) week~u"
		},
		"relative": {
			"hour": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) hours?)~u",
			"minute": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) minutes?)~u",
			"sec": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) seconds?)~u",
			"year": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) years?)~u",
			"week": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) weeks?)~u",
			"day": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) days?)~u",
			"month": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) months?)~u"
		}
	},
