{
    "language": "Spanish",
    "rules": {
        "absolute": {
            "time": "~((?<hour>\\d{1,2})\\:(?<min>\\d{2})(\\:(?<sec>\\d{2}))?)~u",
            "weekday": "~(en|el) (?<pronoun>este|próximo) (?<weekday>lunes|martes|miércoles|jueves|viernes|sábado|domingo)~iu",
            "year": "~(en|el) ((?<pronoun>este|próximo)|((?<digit>[[:digit:]]+))) año~iu",
            "month": "~(en|el) ((?<pronoun>este|próximo) месяце|(?<month>enero|febrero|marzo|abril|mayo|junio|julio|agosto|septiembre|octubre|noviembre|diciembre))~iu",
            "week": "~(en|el) (?<pronoun>este|próximo) semana~iu"
        },
        "relative": {
            "hour": "~(en ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) horas?)~u",
            "minute": "~(en ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) minutos?)~u",
            "sec": "~(en ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) segundos?)~u",
            "year": "~(en ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) años?)~u",
            "week": "~(en ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) semanas?)~u",
            "day": "~(en ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) días?)~u",
            "month": "~(en ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) mes(es)?)~u"
        }
    },
    "week_days": {
        "lunes": "monday",
        "martes": "tuesday",
        "miércoles": "wednesday",
        "jueves": "thursday",
        "viernes": "friday",
        "sábado": "saturday",
        "domingo": "sunday"
    },
    "pronouns": {
        "este": "this",
        "próximo": "next"
    },
    "months": {
        "enero": "january",
        "febrero": "february",
        "marzo": "march",
        "abril": "april",
        "mayo": "may",
        "junio": "june",
        "julio": "july",
        "agosto": "august",
        "septiembre": "september",
        "octubre": "october",
        "noviembre": "november",
        "diciembre": "december"
    },
    "units": {
        "uno": 1,
        "dos": 2,
        "tres": 3,
        "cuatro": 4,
        "cinco": 5,
        "seis": 6,
        "siete": 7,
        "ocho": 8,
        "nueve": 9,
        "diez": 10,
        "once": 11,
        "doce": 12,
        "trece": 13,
        "catorce": 14,
        "quince": 15,
        "dieciseis": 16,
        "diecisiete": 17,
        "dieciocho": 18,
        "diecinueve": 19,
        "veinte": 20
    }
}