{
    "language": "Russian",
    "rules": {
        "absolute": {
            "$year-$month-$digit": "~((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) (?<month>января|февраля|марта|апреля|мая|июня|июля|августа|сентября|октября|ноября|декабря)( (?<year>[[:digit:]]{4})( года)?)?~u",
            "$hour:$min am": [
                "~(?<!через) (?<hour>0?[1-3]|12)\\ ?([\\:.](?<min>\\d{2})| час(а|ов))?\\ ?(ноч(и|ью))~u",
                "~(?<!через) (?<hour>0?[4-9]|1[01])\\ ?([\\:.](?<min>\\d{2})| час(а|ов))?\\ ?(утр(а|ом))~u"
            ],
            "$hour:$min pm": [
                "~(?<!через) (?<hour>0?[1-4]|12)([\\:\\ .](?<min>\\d{2})| час(а|ов))? (дн(я|[её]м))~u",
                "~(?<!через) (?<hour>0?[5-9]|1[01])([\\:\\ .](?<min>\\d{2})| час(а|ов))? (вечер(а|ом))~u"
            ],
            "$hour:00 $timeshift": "~(?<timeshift>(пол(овин[ау])?|(четверть))) (?<hour>\\d{1,2})~u",
            "$hour:00 -$timeshift min": "~без (?<timeshift>\\d{1,2})( минут)? (?<hour>\\d{1,2})~u",
            "$hour:$min": "~(час(а|ов|иков) в (?<hour>\\d{1,2})(\\:(?<min>\\d{2}))?)~u",
            "$hour:$min:$sec": "~((в|около) (?<hour>\\d{1,2})(\\:(?<min>\\d{2}))?(\\:(?<sec>\\d{2}))?)~u",
            "$pronoun week $weekday": "~((?<pronoun>эт(от?|у)|следующ(ий|ее)|прошл(ую|ый))?\\ ?(?<weekday>понедельник|вторник|среду|четверг|пятницу|субботу|воскресен[ьи]е))~u",
            "$pronoun year": "~(в (?<pronoun>этом|следующем|прошлом) году)~u",
            "$digit-$month-$day": "~(в (?<digit>[[:digit:]]+)(-?о?м)? году)~u",
            "$pronoun month": "~(в (?<pronoun>этом|следующем|прошлом) месяце)~u",
            "$year-$month-$day": "~(в (?<month>январе|феврале|марте|апреле|мае|июне|июле|августе|сентябре|октябре|ноябре|декабре))~u",
            "$pronoun week": "~(на (?<pronoun>этой|следующей|прошлой) неделе)~u",
            "9 am": "~утром~u",
            "6 pm": "~вечером~u",
            "9 pm": "~ночью~u",
            "2 pm": "~дн[её]м~u",
            "12 pm": "~в обед~u"
        },
        "relative": {
            "@relative": "~(через(?<relative>((((?<!через)(\\sи|,))?\\s(\\d+|\\pL+(\\s\\pL+)?))\\s(час(а|ов)|мин(ут[уы]?)?|сек(унд[уы]?)?|лет|года?|недел[юиь]|день|дн(я|ей)|месяц(а|ев)?)){2,}))~u",
            "@relative_split": "~(\\sи|,)\\s~u",
            "@relative_join": "через",
            "+$1 hour": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)))? час(а|ов)?)~u",
            "+$1 minute": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)))? мин(ут[уы]?)?)~u",
            "+$1 sec": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)))? сек(унд[уы]?)?)~u",
            "+$1 year": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)))? (лет|года?))~u",
            "+$1 week": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)))? недел[юиь])~u",
            "+$1 day": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)))? (день|дн(я|ей)))~u",
            "+$1 month": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)))? месяц(а|ев)?)~u",
            "-$1 hour": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?час(а|ов)? назад)~u",
            "-$1 minute": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?мин(ут[уы]?)? назад)~u",
            "-$1 sec": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?сек(унд[уы]?)? назад)~u",
            "-$1 year": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?(лет|года?) назад)~u",
            "-$1 week": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?недел[юиь] назад)~u",
            "-$1 day": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?(день|дн(я|ей)) назад)~u",
            "-$1 month": "~((((?<digit>[[:digit:]]+)|(?<alpha>\\pL+( \\pL+)?)) )?месяц(а|ев)? назад)~u",
            "+1 day": "~\\bзавтра\\b~u",
            "+2 day": "~\\bпослезавтра\\b~u",
            "-1 day": "~\\bвчера\\b~u",
            "-2 day": "~\\bпозавчера\\b~u",
            "+0 day": "~\\bсегодня\\b~u"
        }
    },
